{
    "env": {
        "production": {
            "presets": [
                ["env", {
                    "targets": {
                        "browsers": [">2%", "not ie 11", "not op_mini all"]
                    },
                    "modules": false
                }
            ]]
        },
        "development": {
            "presets": [
                ["env", {
                    "targets": {
                        "browsers": [">2%", "not ie 11", "not op_mini all"]
                    },
                    "modules": false
                }
            ]]
        },
        "test": {
            "presets": [
                ["env", {
                    "targets": {"node": "current"}
                }],
                "jest"
            ],
            "plugins": ["transform-es2015-modules-commonjs"]
        }
    }
}
